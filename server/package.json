{
  "name": "stutor-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "prisma": "prisma",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "node src/seed.js",
    "lint": "eslint \"src/**/*.js\" \"tests/**/*.js\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix || true",
    "format": "prettier --write .",
    "test": "cross-env NODE_ENV=test DATABASE_URL=\"file:./test.db\" prisma migrate deploy && node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3"
  }
}
